{
  "name": "@blueflamingos/laravel-client",
  "version": "1.5.0",
  "description": "Laravel client for JavaScript/TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "watch": "tsc --watch",
    "dev": "nodemon --watch src --ext ts,tsx --exec 'npm run build && yalc push'",
    "yalc:publish": "npm run build && yalc publish",
    "yalc:push": "npm run build && yalc push"
  },
  "keywords": [
    "laravel",
    "api"
  ],
  "author": "Blue Flamingos",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blueflamingos/laravel-client.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^22.15.1",
    "nodemon": "^3.1.10",
    "typescript": "^5.8.3"
  }
}
